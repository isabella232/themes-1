{
  "name": "vscode-sourcegraph-theme",
  "displayName": "Sourcegraph Theme",
  "description": "Sourcegraph's theme for Visual Studio Code",
  "version": "0.0.2",
  "publisher": "sourcegraph",
  "bugs": {
    "url": "https://github.com/sourcegraph/themes/issues"
  },
  "homepage": "https://sourcegraph.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcegraph/themes"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Sourcegraph Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/sourcegraph-dark.json"
      }
    ]
  },
  "icon": "icon.png",
  "keywords": [
    "sourcegraph",
    "sourcegraph dark",
    "sourcegraph.com",
    "dark",
    "theme"
  ],
  "scripts": {
    "attach": "./dev.sh attach",
    "eject": "./dev.sh eject",
    "package": "mkdir out && vsce package -o out/vscode-sourcegraph-theme.vsix"
  }
}
